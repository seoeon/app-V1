<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EntryActivity">

    <TextView
        android:id="@+id/EntryActivity"
        android:layout_width="72dp"
        android:layout_height="17dp"
        android:text="EntryActivity"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/SeqActivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SeqActivity"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/EntryActivity" />

    <Button
        android:id="@+id/IntentActivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="IntentActivity"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SeqActivity" />

    <Button
        android:id="@+id/SPActivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SPActivity"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/IntentActivity" />
</androidx.constraintlayout.widget.ConstraintLayout>